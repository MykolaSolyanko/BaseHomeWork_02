// The sum of odd elements.cpp

#include <iostream>
#include <vector>

int main() {
    int n;
    std::cout << "Enter the number of elements in sequence" << std::endl;
    std::cin >> n;
    std::vector<int> a(n);
    std::cout << "Enter the elemnts of sequence" << std::endl;
    for (int i = 0; i < n; ++i)
        std::cin >> a[i];
    int sum = 0;
    for (int i = 0; i < n; ++i)
        if (a[i] % 2 != 0)
            sum += a[i];

    std::cout << "Sum is " << sum << std::endl;

    system("pause");
    return 0;
}
