// Happy ticket.cpp 

#include <iostream>

int main() {
    std::cout << "input 6 digit number " << std::endl;
    int a, b, c, d, e, f;
    std::cin >> a >> b >> c >> d >> e >> f;

    int s1 = a + b + c;
    int s2 = d + e + f;

    if (s1 == s2) {
        std::cout << "Ticket is HAPPY!!!" << std::endl;
    }
    else {
        std::cout << "The ticket is not happy( " << std::endl;
    }
}
